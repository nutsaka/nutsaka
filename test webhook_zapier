integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo=" crossorigin="anonymous"></script>
<script type="text/javascript" src="/assets/slick/slick.min.js"></script>
<script src="/assets/popup/jquery.magnific-popup.min.js"></script>
<script>
$(function () {
$(".navbar-burger").click(function () {
// Toggle the "is-active" class on both the "navbar-burger" and the "navbar-menu"
$(".navbar-burger").toggleClass("is-active");
$(".navbar-menu").toggleClass("is-active");
});
var galleryConfig = {
slidesToShow: 4,
slidesToScroll: 4,
arrow: false,
prevArrow: false,
nextArrow: false,
dots: true,
autoplay: true,
autoplaySpeed: 3000,
responsive: [{
breakpoint: 1024,
settings: {
slidesToShow: 2,
slidesToScroll: 2,
infinite: true,
dots: true
}
},
{
breakpoint: 768,
settings: {
slidesToShow: 2,
slidesToScroll: 2,
dots: true
}
}
]
};
var selectPage = 'galall';
var imgFolder = 'floorplan';
$('.galselect').click(function () {
var page = $(this).attr('data-attr');
$(`#${selectPage}`).hide();
selectPage = page;
$(`#${page}`).show();
$('.gallery').slick('setPosition');
$('.galselect').removeClass('gallery-active')
$(this).addClass('gallery-active');
});
$('.gallery a').magnificPopup({
type: 'image',
removalDelay: 300,
mainClass: 'mfp-fade',
gallery:{
enabled:true
}
});
var fp = ['5th Floor', '6th - 25th Floor', '26th Lower Level', '26th Upper Level',
'27th Lower Level', '27th Upper Level', '28th Floor', '29th Floor', '30th Lower Level',
'30th Upper Level', '31st - 39th Lower Level', '31st - 39th Upper Level',
'40th - 42nd Floor', '43rd Floor'
];
var ul = [
['Loft A1', 'Loft A3', 'Loft B1', 'Loft C1', 'Loft L1', 'Loft L2', 'Loft L3', 'Loft L4',
'Loft L5'
],
['Type A1-1', 'Type A2', 'Type A3', 'Type A4'],
['Type B1', 'Type B2'],
['Type C1', 'Type C2', 'Type C3'],
['Penthouse 1', 'Penthouse 2', 'Penthouse Loft']
];
$.each(fp, function (k, v) {
$('#floorPlan').append(`<option value="${v}">${v}</option>`);
});
$('#floorPlan').change(function(){
var imgName = translate($(this).val());
$('#imgSrc').attr('src',`assets/img/plan/${imgFolder}/${imgName}.jpg`);
});
$('#btnUnitBack').click(function () {
$('#step1').fadeIn();
$('#step2').hide();
$('#floorPlan').html('');
$.each(fp, function (k, v) {
$('#floorPlan').append(`<option value="${v}">${v}</option>`);
});
imgFolder = 'floorplan';
$('#imgSrc').attr('src',`assets/img/plan/${imgFolder}/${translate(fp[0])}.jpg`);
});
$('#btnUnitLayout').click(function () {
$('#step1').hide();
$('#step2').fadeIn();
$('#floorPlan').html('');
$.each(ul[0], function (k, v) {
$('#floorPlan').append(`<option value="${v}">${v}</option>`);
});
imgFolder = 'roomplan';
$('#imgSrc').attr('src',`assets/img/plan/${imgFolder}/${translate(ul[0][0])}.jpg`);
});
$('.btnUnitType').click(function () {
var c = $(this).attr('data-attr');
$('#floorPlan').html('');
$.each(ul[c], function (k, v) {
$('#floorPlan').append(`<option value="${v}">${v}</option>`);
});
$('#imgSrc').attr('src',`assets/img/plan/${imgFolder}/${translate(ul[c][0])}.jpg`);
$('.btnUnitType').removeClass('active');
$(this).addClass('active');
});
$('#btnFactsheet').click(function () {
$('#tb_amenitie').hide();
$('#tb_factsheet').fadeIn();
$('#infoType').hide().fadeIn().html('FACTSHEET');
$('#btnAmenitie').removeClass('active');
$(this).addClass('active');
});
$('#btnAmenitie').click(function () {
$('#tb_amenitie').fadeIn();
$('#tb_factsheet').hide();
$('#infoType').hide().fadeIn().html('AMENITIES');
$('#btnFactsheet').removeClass('active');
$(this).addClass('active');
});
$('.banner').slick({
arrow: false,
dots: true,
prevArrow: false,
nextArrow: false,
infinite: true,
speed: 1000,
cssEase: 'linear',
fade: true,
autoplay: true,
autoplaySpeed: 3000,
});
$('.gallery').slick(galleryConfig);
function translate($name){
$name = $name.split(' ').join('_').toLowerCase();
return $name;
}
});
